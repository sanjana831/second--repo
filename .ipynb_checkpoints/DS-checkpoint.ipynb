   "source": [
    "# Sorting of a list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "e869de5f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[2, 3, 4, 5, 7, 9]\n"
     ]
    }
   ],
   "source": [
    "def Asc_Sort(li):\n",
    "    n = len(li)\n",
    "    for i in range(n):\n",
    "        for j in range(n-i-1):\n",
    "            if li[j] > li[j+1]:\n",
    "                li[j],li[j+1]=li[j+1],li[j]\n",
    "    return li\n",
    "            \n",
    "\n",
    "li = [7,3,5,2,9,4]\n",
    "res = Asc_Sort(li)\n",
    "print(res)"
   ]
